 <div id="main">
    <div id="page1">
       
          <div id="register-container">
            <div id="top">
              <h1>REGISTER</h1>
            </div>
            <form id="form"action="/register" method="POST">
              
              <label id="txt" for="name">Full Name:</label>
              <input type="text" id="name" name="name" placeholder="Enter your full name" required>
              <br><br>
        
             
              <label id="txt" for="email">Email:</label>
              <input type="email" id="email" name="email" placeholder="Enter your email" required>
              <br><br>
        
              
              <label id="txt" for="password">Password:</label>
              <input type="password" id="password" name="password" placeholder="Enter your password" required>
              <br><br>
        
              
              <button id="esubmit" type="submit">Register</button>
            </form>
            <div id="ftext"><h3>Already Have An Account? <a href="/login">Login</a></h3></div>
          </div>
    </div>


</div>
